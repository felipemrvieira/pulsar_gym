<div id="<%= dom_id address %>">
  <p>
    <strong>Street name:</strong>
    <%= address.street_name %>
  </p>

  <p>
    <strong>Building number:</strong>
    <%= address.building_number %>
  </p>

  <p>
    <strong>Complement:</strong>
    <%= address.complement %>
  </p>

  <p>
    <strong>Neighbor:</strong>
    <%= address.neighbor_id %>
  </p>

</div>
